doctype html
html
  head
    meta(charset='utf-8')
    title People-De-Color
    link(rel='stylesheet', href='/css/style.css')
  body
    h1.header People-de-Color
    img.globe(src='/img/512px-Globe_font_awesome.svg.png')
    nav
      ul
        li: a(href='/') Home
        li: a(href='/about') About
        li: a(href='/faq') FAQ
        li: a(href='/contact') Contact info
        li: a(href='/people') People Info

    h2 People Page!

    each person in people
        .personDiv
          .person
          h3.personName: a(href='person.website').divClick=  person.name
          ul.list
            li= person.email
            li= person.color
            li= person.website
            li= person.phone

